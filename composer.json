{
    "name": "freshp/phpunit-webtestcase-fixture-helper",
    "description": "Helps to manage fixtures for test-environment in webtestcases.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "symfony/framework-bundle": "^4.3",
        "symfony/browser-kit": "^4.3"
    },
    "require-dev": {
        "tm/tooly-composer-script": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "FreshP\\PhpunitWebtestcaseFixtureHelper\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Tooly\\ScriptHandler::installPharTools"
        ],
        "post-update-cmd": [
            "Tooly\\ScriptHandler::installPharTools"
        ]
    },
    "extra": {
        "tools": {
            "phpunit": {
                "url": "https://phar.phpunit.de/phpunit-8.2.5.phar",
                "sign-url": "https://phar.phpunit.de/phpunit-8.2.5.phar.asc"
            },
            "phpcs-fixer": {
                "url": "https://github.com/FriendsOfPHP/PHP-CS-Fixer/releases/download/v2.15.1/php-cs-fixer.phar",
                "force-replace": true
            }
        }
    }
}
